# Server Configuration
MD_SERVER_HOST=127.0.0.1
MD_SERVER_PORT=8080
MD_SERVER_API_KEY=
MD_SERVER_MAX_FILE_SIZE=52428800
MD_SERVER_TIMEOUT_SECONDS=30
MD_SERVER_DEBUG=true

# Proxy Configuration (optional)
MD_SERVER_HTTP_PROXY=http://<your-proxy>.example.com:3128
MD_SERVER_HTTPS_PROXY=https://<your-proxy>.example.com:3128

# External Service Configuration (optional)
MD_SERVER_OPENAI_API_KEY=
MD_SERVER_AZURE_DOC_INTEL_ENDPOINT=https://<your-service>.cognitiveservices.azure.com
MD_SERVER_AZURE_DOC_INTEL_KEY=

# Crawl4AI Configuration (optional - has sensible defaults)
# MD_SERVER_CRAWL4AI_ENABLED=true
# MD_SERVER_CRAWL4AI_JS_RENDERING=false
# MD_SERVER_CRAWL4AI_TIMEOUT=30
# MD_SERVER_CRAWL4AI_USER_AGENT=  # Leave empty to use Crawl4AI default

# LLM integration (optional - only needed for AI filtering)
# MD_SERVER_LLM_PROVIDER_URL=https://openrouter.ai/api/v1
# MD_SERVER_LLM_API_KEY=your_key
# MD_SERVER_LLM_MODEL=google/gemini-2.5-flash

# Alternative: OpenAI direct
# MD_SERVER_LLM_PROVIDER_URL=https://api.openai.com/v1
# MD_SERVER_LLM_MODEL=gpt-4o-mini